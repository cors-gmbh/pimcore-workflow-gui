{
  "name": "cors/workflow-gui",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Workflow Configuration UI for Pimcore",
  "keywords": [
    "pimcore",
    "imports",
    "pimcore-plugin",
    "pimcore-bundle"
  ],
  "homepage": "https://github.com/cors-gmbh/pimcore-workflow-gui",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "pimcore/pimcore": "^10.6.0 | ^11.2"
  },
  "autoload": {
    "psr-4": {
      "CORS\\Pimcore\\WorkflowGui\\": "src/WorkflowGui"
    }
  },
  "autoload-dev": {
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "CORS\\Pimcore\\WorkflowGui\\WorkflowGuiBundle"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^11.1",
    "symfony/dotenv": "^7.0",
    "pimcore/admin-ui-classic-bundle": "^1.4"
  }
}
